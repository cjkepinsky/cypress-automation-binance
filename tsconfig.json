{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node", "cypress-real-events"],
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "config/*": ["cypress/config/*"],
      "consts/*": ["cypress/consts/*"],
      "datasets/*": ["cypress/datasets/*"],
      "helpers/*": ["cypress/helpers/*"],
      "pages/*": ["cypress/pages/*"],
      "routes/*": ["cypress/routes/*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "include": ["cypress/**/*.ts"]
}